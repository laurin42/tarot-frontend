{
  "name": "tarot",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:dev": "dotenv -e .env.development -- expo start",
    "start:prod": "dotenv -e .env.production -- expo start",
    "android:dev": "dotenv -e .env.development -- expo start --android",
    "android:prod": "dotenv -e .env.production -- expo start --android",
    "ios:dev": "dotenv -e .env.development -- expo start --ios",
    "ios:prod": "dotenv -e .env.production -- expo start --ios",
    "web:dev": "dotenv -e .env.development -- expo start --web",
    "web:prod": "dotenv -e .env.production -- expo start --web",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "DARK_MODE=media expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "server": "cd ../backend && npm run dev",
    "concurrent": "concurrently \"npm run server\" \"npm run start\"",
    "bugsnag:upload": "bugsnag-expo upload-sourcemaps",
    "android:dev-client": "expo run:android --dev-client",
    "android:dev:dev-client": "dotenv -e .env.development -- expo run:android --dev-client",
    "android:prod:dev-client": "dotenv -e .env.production -- expo run:android --dev-client"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
